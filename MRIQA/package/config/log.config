[loggers]
keys=root,main,unnamed

[handlers]
keys=fh,sh,unnm

[formatters]
keys=form0

[logger_root]
level=INFO
handlers=fh
formatter=form0

[logger_main]
level=DEBUG
handlers=sh
formatter=form0
qualname=main

[logger_unnamed]
level=DEBUG
handlers=unnm
formatter=form0
qualname=unnamed

[handler_fh]
class=FileHandler
level=DEBUG
formatter=form0
args=(os.path.join(os.curdir, image_handling_log),'w')

[handler_unnm]
class=FileHandler
level=DEBUG
formatter=form0
args=(os.path.join(os.curdir, unnamed_log),'w')

[handler_sh]
class=StreamHandler
level=DEBUG
formatter=form0
args=(sys.stderr,)

[formatter_form0]
format=%(asctime)s - %(filename)s - %(levelname)s - %(message)s
datefmt=
class=logging.Formatter
